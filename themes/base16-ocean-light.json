                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "keyword.other.unit",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#d08770"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
