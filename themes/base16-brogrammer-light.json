                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "keyword.other.unit",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#de352e"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
