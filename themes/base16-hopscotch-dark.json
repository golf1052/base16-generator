            ],
            "settings": {
                "foreground": "#dd464c"
            }
        },
        {
            "name": "Properties",
            "scope": [
                "variable.other.object.property"
            ],
            "settings": {
                "foreground": "#1290bf"
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#8fc13e"
            }
        },
        {
            "name": "Invalid",
            "scope": [
