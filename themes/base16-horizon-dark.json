                "storage.type.primitive"
            ],
            "settings": {
                "foreground": "#24A8B4"
            }
        },
        {
            "name": "Function",
            "scope": [
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#DF5273"
            }
        },
