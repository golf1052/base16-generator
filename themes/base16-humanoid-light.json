            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#388e3c"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "support.other.namespace.use.php",
